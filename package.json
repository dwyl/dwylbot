  "devDependencies": {
    "babel-brunch": "~6.0.0",
    "brunch": "2.7.4",
    "clean-css-brunch": "~2.0.0",
    "css-brunch": "~2.0.0",
    "javascript-brunch": "~2.0.0",
    "pre-commit": "^1.2.2",
    "uglify-js-brunch": "~2.0.1"
  },
  "pre-commit": [
    "coverage",
    "lint"
  ]
